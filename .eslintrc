{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-unused-vars": 0,
    "no-constant-condition" : 0,
    "no-param-reassign": 0,
    "no-useless-return": 0,
    "no-return-assign": 0,
    "no-restricted-syntax": 0,
    "no-restricted-globals": 0,
    "guard-for-in": 0,
    "no-shadow": 0,
    "no-alert": 0,
    "max-classes-per-file": 0,
    "indent": 0,
    "no-multiple-empty-lines": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "no-undef": 0,
    "class-methods-use-this": 0
  }
}
